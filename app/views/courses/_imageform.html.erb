<%= form_for(@course, html: {multipart: true}) do |f| %>
	  <% if @course.image.exists? %>
	    <div class="text-center">
      		<%= image_tag @course.image.url %><br/>
      	</div>
		<div>
  	  	<% else %>
		  	<%= f.label :image, "Attach an Image" %><br />
		  	<%= f.file_field :image %>
		  	<div>
		  	<%= f.button :submit, class:'btn btn-default' %>
		  	</div>
		<% end %>
		</div>

<% end %>

